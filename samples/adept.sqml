User {
    id:          sequence('user_id', 1),
    firstname:   firstName(),
    lastname:    lastName(),
    age:         random(18, 24)
}*20;

Job {
    trackingNumber: sequence('tn', 1),
    createdDate:    date(),
    requester:      `select id from User where id = %@ AND (firstname = 'Higgor' OR age > %@)` [random(1,20), 20]
} * 10;